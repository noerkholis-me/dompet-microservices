datasource db {
  provider = "postgresql"
}

model Transaction {
  id              String         @id @default(uuid())
  kodeBilling     String         @unique
  pembeliId       String         
  totalHarga      Float
  status          TransactionStatus  @default(BELUM_DIBAYAR)
  expiredAt       DateTime
  createdAt       DateTime       @default(now())
  updatedAt       DateTime       @updatedAt

  items           CartItem[]     

  @@map("transaksi")
}

model CartItem {
  id             String      @id @default(uuid())
  transactionId  String
  productId      String      
  harga          Float       

  transaction    Transaction @relation(fields: [transactionId], references: [id], onDelete: Cascade)

  @@map("keranjang")
}

enum TransactionStatus {
  BELUM_DIBAYAR
  SUDAH_DIBAYAR
}
