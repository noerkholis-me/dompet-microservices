  email      String    @unique
  password   String    
  status     Boolean   @default(true)  
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt

  roles      UserRole[]  

  @@map("users")
}

model UserRole {
  id        String   @id @default(uuid())
  userId    String
  role      RoleType

  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, role])
  @@map("users_role")
}

